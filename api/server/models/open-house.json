{
  "name": "OpenHouse",
  "base": "PersistedModel",
  "options": {
    "validateUpsert": true,
    "postgresql": {
      "schema": "spiti",
      "table": "open_house"
    }
  },
  "mixins": {
    "RemoteRouting" : {
      "only": [
        "@find",
        "@findById",
        "updateAttributes",
        "__get__images",
        "__link__images",
        "__unlink__images"
      ]
    },
    "CreateWithRelationsLink": {
      "images": {
        "checkOwner": true,
        "removeOldRelations": true 
      }
    },
    "ReadOnly" : {
      "userId": true,
      "feedId" : true
    }
  },
  "properties": {
    "id": {
      "type": "number",
      "id": true
    },
    "userId": {
      "type": "number",
      "postgresql": {
        "columnName": "userId",
        "dataType": "integer"
      }
    },
    "feedId": {
      "type": "number",
      "postgresql": {
        "columnName": "feedId",
        "dataType": "integer"
      }
    },
    "host": {
      "type": "string"
    },
    "contactPhone": {
      "type": "string",
      "min": 0,
      "postgresql": {
        "columnName": "contactPhone",
        "dataType": "character varying",
        "dataLength": 100
      }
    },
    "date": {
      "type": "date",
      "postgresql": {
        "dataType": "date"
      }
    },
    "timeStart": {
      "type": "date",
      "postgresql": {
        "columnName": "timeStart",
        "dataType": "time without time zone"
      }
    },
    "timeEnd": {
      "type": "date",
      "postgresql": {
        "columnName": "timeEnd",
        "dataType": "time without time zone"
      }
    }
  },
  "validations": [],
  "relations": {
    "feed": {
      "type": "belongsTo",
      "model": "Feed",
      "foreignKey": "feedId"
    },
    "account": {
      "type": "belongsTo",
      "model": "Account",
      "foreignKey": "userId"
    },
    "images": {
      "type": "hasMany",
      "model": "Attachment",
      "through": "AttachmentToOpenHouse",
      "foreignKey": "openHouseId",
      "keyThrough": "attachmentId"
    }
  },
  "acls": [
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    }
  ],
  "methods": []
}
