{
  "name": "FavoriteListing",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "postgresql": {
      "table": "favorite_listing",
      "schema": "spiti"
    }
  },
  "mixins": {
    "RelationsValidate": {
      "listing": {
        "checkNotOwner": true
      }
    }
  },
  "properties": {
    "user_id": {
      "type": "number",
      "required": true
    },
    "listing_id": {
      "type": "number",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "client": {
      "type": "belongsTo",
      "model": "Client",
      "foreignKey": "user_id"
    },
    "listing": {
      "type": "belongsTo",
      "model": "Listing",
      "foreignKey": "listing_id"
    }
  },
  "acls": [
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    }
  ],
  "methods": {}
}
