    "userId": {
      "type": "number",
      "required": true,
      "postgresql": {
        "columnName": "userId"
      }
    },
    "chatId": {
      "type": "number",
      "required": true,
      "postgresql": {
        "columnName": "chatId"
      }
    },
    "lastReadedMessageId": {
      "type": "number",
      "postgresql": {
        "columnName": "lastReadedMessageId"
      }
    }
  },
  "validations": [],
