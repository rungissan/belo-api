{
  "name": "Account",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "postgresql": {
      "schema": "spiti",
      "table": "account"
    }
  },
  "mixins": {
    "Timestamp": {},
    "ReadOnly" : {
      "type" : true
    },
    "RemoteRouting" : {
      "only": [
        "@find",
        "@findById",
        "updateAttributes",
        "__get__background",
        "__get__avatar",
        "__get__favoriteFeeds",
        "__link__favoriteFeeds",
        "__unlink__favoriteFeeds",
        "__get__savedFeeds",
        "__get__followed",
        "__link__followedAccount",
        "__unlink__followedAccount",
        "__get__connection"
      ]
    },
    "AjvValidation": {
      "remoteHooks": {
        "prototype.patchAttributes": "patch-attributes"
      }
    },
    "RelationsValidate": {
      "avatar": {
        "checkOwner": true
      },
      "background": {
        "checkOwner": true
      }
    }
  },
  "properties": {
    "userId": {
      "type": "number",
      "id": true,
      "postgresql": {
        "columnName": "userId"
      }
    },
    "type": {
      "type": "string",
      "postgresql": {
        "dataType": "character varying",
        "dataLength": 20
      }
    },
    "firstName": {
      "type": "string",
      "minLength": 1,
      "maxLength": 30,
      "postgresql": {
        "columnName": "firstName",
        "dataType": "character varying",
        "dataLength": 30
      }
    },
    "lastName": {
      "type": "string",
      "minLength": 1,
      "maxLength": 30,
      "postgresql": {
        "columnName": "lastName",
        "dataType": "character varying",
        "dataLength": 30
      }
    },
    "userName": {
      "type": "string",
      "minLength": 1,
      "maxLength": 30,
      "postgresql": {
        "columnName": "userName",
        "dataType": "character varying",
        "dataLength": 30
      }
    },
    "phone": {
      "type": "string",
      "minLength": 1,
      "maxLength": 30,
      "postgresql": {
        "dataType": "character varying",
        "dataLength": 30
      }
    },
    "about": {
      "type": "string"
    },
    "biography": {
      "type": "string"
    },
    "brokerage": {
      "type": "string",
      "minLength": 1,
      "maxLength": 100,
      "postgresql": {
        "dataType": "character varying",
        "dataLength": 100
      }
    },
    "licenseType": {
      "type": "string",
      "maxLength": 50,
      "postgresql": {
        "columnName": "licenseType",
        "dataLength": 50
      }
    },
    "licenseState": {
      "type": "string",
      "maxLength": 4,
      "postgresql": {
        "columnName": "licenseState",
        "dataLength": 4
      }
    },
    "licenseNumber": {
      "type": "string",
      "maxLength": 50,
      "postgresql": {
        "columnName": "licenseNumber",
        "dataType": "character varying",
        "dataLength": 50
      }
    },
    "licenseExpiration": {
      "type": "date",
      "postgresql": {
        "columnName": "licenseExpiration"
      }
    },
    "avatarId": {
      "type": "number",
      "min": 1,
      "postgresql": {
        "columnName": "avatarId",
        "dataType": "integer"
      }
    },
    "backgroundId": {
      "type": "number",
      "min": 1,
      "postgresql": {
        "columnName": "backgroundId",
        "dataType": "integer"
      }
    }
  },
  "validations": [],
  "relations": {
    "client": {
      "type": "belongsTo",
      "model": "Client",
      "foreignKey": "userId"
    },
    "avatar": {
      "type": "belongsTo",
      "model": "Attachment",
      "foreignKey": "avatarId"
    },
    "background": {
      "type": "belongsTo",
      "model": "Attachment",
      "foreignKey": "backgroundId"
    },
    "geolocations": {
      "type": "hasMany",
      "model": "Geolocation",
      "through": "GeolocationToAccount",
      "foreignKey": "userId",
      "keyThrough": "geolocationId"
    },
    "favoriteFeeds": {
      "type": "hasMany",
      "model": "Feed",
      "through": "FavoriteFeed",
      "foreignKey": "userId",
      "keyThrough": "feedId"
    },
    "savedFeeds": {
      "type": "hasMany",
      "model": "SavedFeed",
      "foreignKey": "userId"
    },
    "followedAccount": {
      "type": "hasMany",
      "model": "Account",
      "through": "Followed",
      "foreignKey": "userId",
      "keyThrough": "followedId"
    },
    "followed": {
      "type": "hasMany",
      "model": "Followed",
      "foreignKey": "userId"
    },
    "connection": {
      "type": "hasMany",
      "model": "Connection",
      "foreignKey": "userId"
    },
    "appointment": {
      "type": "hasMany",
      "model": "Appointment",
      "foreignKey": "userId"
    },
    "statusCheck": {
      "type": "hasMany",
      "model": "StatusCheck",
      "foreignKey": "userId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__get__savedFeeds"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "setGeolocation"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "search"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "preview"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "getOwnSortedOpenHouses"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "getFavoriteSortedOpenHouses"
    }
  ],
  "methods": []
}
