{
  "name": "Account",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "postgresql": {
      "schema": "spiti"
    }
  },
  "mixins": {
    "Timestamp": {},
    "ReadOnly" : {
      "type" : true
    },
    "AjvValidation": {
      "remoteHooks": {
        "prototype.patchAttributes": "patch-attributes"
      }
    },
    "RelationsValidate": {
      "avatar": {
        "checkOwner": true
      },
      "background": {
        "checkOwner": true
      }
    }
  },
  "properties": {
    "userId": {
      "type": "number",
      "id": true
    },
    "type": {
      "type": "string",
      "postgresql": {
        "dataType": "character varying",
        "dataLength": 20
      }
    },
    "firstName": {
      "type": "string",
      "minLength": 2,
      "maxLength": 30,
      "postgresql": {
        "columnName": "first_name",
        "dataType": "character varying",
        "dataLength": 30
      }
    },
    "lastName": {
      "type": "string",
      "minLength": 2,
      "maxLength": 30,
      "postgresql": {
        "columnName": "last_name",
        "dataType": "character varying",
        "dataLength": 30
      }
    },
    "userName": {
      "type": "string",
      "minLength": 2,
      "maxLength": 30,
      "postgresql": {
        "columnName": "username",
        "dataType": "character varying",
        "dataLength": 30
      }
    },
    "phone": {
      "type": "string",
      "minLength": 3,
      "maxLength": 30,
      "postgresql": {
        "dataType": "character varying",
        "dataLength": 30
      }
    },
    "about": {
      "type": "string"
    },
    "biography": {
      "type": "string"
    },
    "brokerage": {
      "type": "string",
      "maxLength": 100,
      "postgresql": {
        "dataType": "character varying",
        "dataLength": 100
      }
    },
    "licenseType": {
      "type": "string",
      "maxLength": 50,
      "postgresql": {
        "columnName": "license_type",
        "dataLength": 50
      }
    },
    "licenseState": {
      "type": "string",
      "maxLength": 4,
      "postgresql": {
        "columnName": "license_state",
        "dataLength": 4
      }
    },
    "licenseNumber": {
      "type": "number",
      "max": 50,
      "postgresql": {
        "columnName": "license_number",
        "dataLength": 50
      }
    },
    "licenseExpiration": {
      "type": "date",
      "postgresql": {
        "columnName": "license_expiration"
      }
    },
    "avatarId": {
      "type": "number",
      "min": 1,
      "postgresql": {
        "columnName": "avatar_id",
        "dataType": "integer"
      }
    },
    "backgroundId": {
      "type": "number",
      "min": 1,
      "postgresql": {
        "columnName": "background_id",
        "dataType": "integer"
      }
    }
  },
  "validations": [],
  "relations": {
    "client": {
      "type": "belongsTo",
      "model": "Client",
      "foreignKey": "userId"
    },
    "avatar": {
      "type": "belongsTo",
      "model": "Attachment",
      "foreignKey": "avatarId"
    },
    "background": {
      "type": "belongsTo",
      "model": "Attachment",
      "foreignKey": "backgroundId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    }
  ],
  "methods": []
}
