{
  "name": "Listing",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "postgresql": {
      "schema": "spiti"
    }
  },
  "mixins": {
    "Timestamp": {},
    "SetOwner": {},
    "RelationsValidate": {
      "image": {
        "checkOwner": true
      },
      "geolocation": {}
    },
    "RemoteRouting" : {
      "only": [
        "@find",
        "@findById",
        "@create",
        "updateAttributes",
        "__link__additionalImages",
        "__unlink__additionalImages"
      ]
    }
  },
  "properties": {
    "id": {
      "type": "number",
      "id": true
    },
    "userId": {
      "type": "number",
      "postgresql": {
        "columnName": "user_id",
        "dataType": "integer"
      }
    },
    "title": {
      "type": "string",
      "maxLength": 255,
      "postgresql": {
        "dataType": "character varying"
      }
    },
    "description": {
      "type": "string"
    },
    "imageId": {
      "type": "number",
      "min": 1,
      "postgresql": {
        "columnName": "image_id",
        "dataType": "integer"
      }
    },
    "geolocationId": {
      "type": "number",
      "min": 1,
      "postgresql": {
        "columnName": "geolocation_id",
        "dataType": "integer"
      }
    },
    "displayAddress": {
      "type": "boolean",
      "postgresql": {
        "columnName": "display_address"
      }
    },
    "showInBrokerFeed": {
      "type": "boolean",
      "postgresql": {
        "columnName": "show_in_broker_feed"
      }
    },
    "rent_type": {
      "type": "boolean",
      "default": "rent",
      "postgresql": {
        "columnName": "rent_type",
        "dataType": "character varying",
        "dataLength": 20
      }
    },
    "bedrooms": {
      "type": "number",
      "min": 0,
      "postgresql": {
        "dataType": "smallint"
      }
    },
    "bathrooms": {
      "type": "number",
      "min": 0,
      "postgresql": {
        "dataType": "smallint"
      }
    },
    "price": {
      "type": "number",
      "min": 0,
      "postgresql": {
        "dataType": "integer"
      },
      "description": "Price in cents"
    },
    "square": {
      "type": "number"
    },
    "propertyFeatures": {
      "type": "object",
      "postgresql": {
        "columnName": "property_features",
        "dataType": "jsonb"
      }
    },
    "buildingFeatures": {
      "type": "object",
      "postgresql": {
        "columnName": "building_features",
        "dataType": "jsonb"
      }
    },
    "utilitiesIncluded": {
      "type": "object",
      "postgresql": {
        "columnName": "utilities_included",
        "dataType": "jsonb"
      }
    },
    "moveInFees": {
      "type": "object",
      "postgresql": {
        "columnName": "move_in_fees",
        "dataType": "jsonb"
      }
    },
    "schoolInformation": {
      "type": "object",
      "postgresql": {
        "columnName": "school_information",
        "dataType": "jsonb"
      }
    },
    "transportation": {
      "type": "object",
      "postgresql": {
        "dataType": "jsonb"
      }
    },
    "additionalFeatures": {
      "type": "object",
      "postgresql": {
        "columnName": "additional_features",
        "dataType": "jsonb"
      }
    }
  },
  "validations": [],
  "relations": {
    "client": {
      "type": "belongsTo",
      "model": "Client",
      "foreignKey": "userId"
    },
    "image": {
      "type": "belongsTo",
      "model": "Attachment",
      "foreignKey": "imageId"
    },
    "additionalImages": {
      "type": "hasMany",
      "model": "Attachment",
      "through": "AttachmentToPost",
      "foreignKey": "post_id",
      "keyThrough": "attachment_id"
    },
    "geolocation": {
      "type": "belongsTo",
      "model": "Geolocation",
      "foreignKey": "geolocationId"
    }
  },
  "acls": [
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    },
    {
      "principalType": "ROLE",
      "principalId": "prof",
      "permission": "ALLOW",
      "property": "create"
    }
  ],
  "methods": []
}
