{
  "name": "ChatMessage",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "postgresql": {
      "schema": "spiti",
      "table": "chat_message"
    }
  },
  "mixins": {
    "Timestamp": {},
    "RelationsValidate": {
      "chat": {}
    },
    "ReadOnly" : {
      "accountId": true,
      "chatId" : true
    }
  },
  "properties": {
    "id": {
      "type": "number",
      "id": true
    },
    "chatId": {
      "type": "number",
      "min": 1,
      "postgresql": {
        "columnName": "chatId",
        "dataType": "integer"
      }
    },
    "userId": {
      "type": "number",
      "min": 1,
      "postgresql": {
        "columnName": "userId",
        "dataType": "integer"
      }
    },
    "message": {
      "type": "object",
      "postgresql": {
        "columnName": "message",
        "dataType": "jsonb"
      }
    }
  },
  "validations": [],
  "relations": {
    "account": {
      "type": "belongsTo",
      "model": "Account",
      "foreignKey": "userId"
    },
    "chat": {
      "type": "belongsTo",
      "model": "Chat",
      "foreignKey": "chatId"
    },
    "chatToAccount": {
      "type": "belongsTo",
      "model": "ChatToAccount",
      "foreignKey": "chatId",
      "primaryKey": "chatId"
    }
  },
  "acls": [
  ],
  "methods": []
}
